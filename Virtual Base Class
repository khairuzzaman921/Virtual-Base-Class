#include <iostream>

using namespace std;


    class A
    {
        public:
        void display(void);
    };

    class B1:virtual public A
    {

    };

    class B2:virtual public A
    {

    };

    class C :public B1,public B2
    {

    };

    int main()
    {
        C m;
        m.display();

    }
